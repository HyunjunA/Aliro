{
  "name": "Aliro-webapp",
  "version": "1.0.0",
  "description": "your friendly machine learning assistant",
  "scripts": {
    "build": "webpack --mode=production",
    "build-dev": "webpack --watch --mode=development",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EpistasisLab/Aliro.git"
  },
  "keywords": [
    "machine learning"
  ],
  "author": "University of Pennsylvania",
  "license": "GPL 3.0",
  "bugs": {
    "url": "https://github.com/EpistasisLab/Aliro/issues"
  },
  "homepage": "https://github.com/EpistasisLab/Aliro",
  "dependencies": {
    "array-move": "^3.0.0",
    "c3": "^0.4.24",
    "core-js": "^3.6.5",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.25.3",
    "papaparse": "^5.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-redux": "^5.1.2",
    "react-responsive": "^8.0.3",
    "react-router": "^3.2.6",
    "react-sortable-hoc": "^1.11.0",
    "readable-stream": "^4.5.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.88.2",
    "socket.io-client": "^2.3.0",
    "twix": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@jest/core": "^25.5.4",
    "@types/jest": "^29.5.13",
    "assert": "^2.1.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.1.0",
    "cheerio": "^1.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.8",
    "enzyme-to-json": "^3.6.2",
    "fetch-mock": "^7.7.3",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "http-browserify": "^1.7.0",
    "jest": "^29.7.0",
    "jest-circus": "^25.5.4",
    "jest-environment-node": "^29.7.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-html-reporter": "^2.8.2",
    "jest-junit": "^6.4.0",
    "jest-react-native": "^18.0.0",
    "net-browserify": "^0.2.4",
    "node-libs-browser": "^2.2.1",
    "postcss": "^8.4.20",
    "pretty-format": "^29.7.0",
    "react-native": "^0.75.4",
    "react-test-renderer": "^16.13.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.2.6",
    "typescript": "^3.9.2",
    "undici": "^6.20.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^node:stream$": "stream-browserify",
      "^assert$": "assert/",
      "^node:assert$": "assert/",
      "^node:net$": "net-browserify/",
      "^net$": "net-browserify/",
      "^http$": "http-browserify/lib/request",
      "^node:http$": "http-browserify/lib/request",
      "^node:(.*)$": "<rootDir>/node_modules/$1"
    },
    "moduleDirectories": [
      "node_modules",
      "/appsrc/lab/webapp/node_modules",
      "/appsrc/lab",
      "/appsrc",
      "<rootDir>"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.ts?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(undici|fetch-mock|parse5-parser-stream|stream|assert|stream-browserify|readable-stream|net-browserify|http-browserify|https-browserify)/)"
    ],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Jest Webapp Unit Test Suite",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "outputPath": "../../target/test-reports/html/unit_webapp_jest_test_report.html"
        }
      ],
      [
        "./node_modules/jest-junit",
        {
          "suiteName": "Jest Unit Test Suite",
          "outputDirectory": "/target/test-reports",
          "outputName": "unit_jest_xunit.xml",
          "classNameTemplate": "JestWebUnit.{classname}",
          "titleTemplate": "{title}"
        }
      ]
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node",
    "testEnvironmentOptions:": {},
    "resolver": "<rootDir>/jest-resolver.js"
  }
}
