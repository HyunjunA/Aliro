{
  "name": "Aliro-webapp",
  "version": "1.0.0",
  "description": "your friendly machine learning assistant",
  "scripts": {
    "build": "webpack --mode=production",
    "build-dev": "webpack --watch --mode=development",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EpistasisLab/Aliro.git"
  },
  "keywords": ["machine learning"],
  "author": "University of Pennsylvania",
  "license": "GPL 3.0",
  "bugs": {
    "url": "https://github.com/EpistasisLab/Aliro/issues"
  },
  "homepage": "https://github.com/EpistasisLab/Aliro",
  "dependencies": {
    "array-move": "^3.0.0",
    "c3": "^0.4.24",
    "core-js": "^3.6.5",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^3.0.0",
    "moment": "^2.30.1",
    "papaparse": "^5.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.0.1",
    "react-redux": "^5.1.2",
    "react-responsive": "^8.0.3",
    "react-router": "^3.2.6",
    "react-sortable-hoc": "^1.11.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "semantic-ui-react": "^0.88.2",
    "socket.io-client": "^4.8.0",
    "twix": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.25.7",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "autoprefixer": "^10.4.13",
    "babel-jest": "26.6.3",
    "babel-loader": "^9.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.6.1",
    "fetch-mock": "^11.1.5",
    "html-webpack-plugin": "^5.6.2",
    "html-webpack-template": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "26.6.3",
    "jest-environment-jsdom": "26.6.3",
    "jest-fetch-mock": "^2.1.2",
    "jest-html-reporter": "^3.7.0",
    "jest-junit": "^16.0.0",
    "jest-react-native": "^18.0.0",
    "postcss": "^8.4.20",
    "react-native": "^0.75.4",
    "react-test-renderer": "^16.13.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.14.1",
    "tailwindcss": "^3.2.6",
    "typescript": "^3.9.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.js?$": "babel-jest",
      "^.+\\.ts?$": "babel-jest"
    },
    "transformIgnorePatterns": ["node_modules/(?!(fetch-mock)/)"],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Jest Webapp Unit Test Suite",
          "includeFailureMsg": true,
          "includeConsoleLog": true,
          "outputPath": "../../target/test-reports/html/unit_webapp_jest_test_report.html"
        }
      ],
      [
        "./node_modules/jest-junit",
        {
          "suiteName": "Jest Unit Test Suite",
          "outputDirectory": "/target/test-reports",
          "outputName": "unit_jest_xunit.xml",
          "classNameTemplate": "JestWebUnit.{classname}",
          "titleTemplate": "{title}"
        }
      ]
    ]
  }
}
